    "@types/chai": "^4.3.4",
    "@types/md5": "^2.3.2",
    "@types/mocha": "^10.0.1",
    "@types/pg": "^8.6.6",
    "@types/signale": "^1.4.4",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.4"
  }
